generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Database {
  id               String @id @default(uuid())
  name             String
  state            String
  provider         String
  type             String
  connectionString String
  userId           String
}
